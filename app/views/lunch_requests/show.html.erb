
<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-10 col-sm-offset-2">
      <div class="content">
        <h1>Your request is waiting for a match!</h1>
        <p>Your <%= @lunch_request.lunch_type %> lunch request for <%= @lunch_request.suggested_duration %> minutes at <%= @lunch_request.datetime %></p>
      </div>
      <div class="sk-cube-grid">
        <div class="sk-cube sk-cube1"></div>
        <div class="sk-cube sk-cube2"></div>
        <div class="sk-cube sk-cube3"></div>
        <div class="sk-cube sk-cube4"></div>
        <div class="sk-cube sk-cube5"></div>
        <div class="sk-cube sk-cube6"></div>
        <div class="sk-cube sk-cube7"></div>
        <div class="sk-cube sk-cube8"></div>
        <div class="sk-cube sk-cube9"></div>
      </div>
    </div>
  </div>
</div>

<% if @lunch_request.lunch_date %>
<%= @lunch_request.lunch_date %>
<%= lunch_request.lunch_date.begin %>
<%= lunch_request.lunch_date.suggested_duration %>

<% end %>


<% content_for :after_js do %>
  <script>
    console.log("my script runs");
    App['incoming_requests'] = App.cable.subscriptions.create(
      { channel: 'IncomingRequestsChannel' },
      { received: (data) =>

        console.log(data)
      }
    );
  </script>
<% end %>
